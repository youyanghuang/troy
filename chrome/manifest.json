{
  "manifest_version": 2,

  "name": "Troy",
  "description": "Remote control chrome",
  "version": "1.0",
  "icons": { "256": "icon.png" },
  "background": {
    "scripts": ["main.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Troy 代理"
  },
  "permissions": [
    "proxy",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}